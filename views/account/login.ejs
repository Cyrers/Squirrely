<h1>Login</h1>
<form action="login" method="POST">
  <div>
    <input
      type="text"
      id="username"
      name="username"
      placeholder="Username"
      required
    />
  </div>
  <div>
    <input
      type="password"
      id="password"
      name="password"
      placeholder="password"
      required
    />
  </div>
  <button type="submit">Login</button>
</form>
<!-- Redirect to register page-->
<a href="register">Register</a>
